string = input("Enter a string: ")
count_vowels = 0
count_consonents = 0
for i in string:
    if i == "a" or i == "A" or i == "e" or i == "E" or i == "o" or i == "O" or i == "i" or i == "I" or i == "u" or i == "U":
        count_vowels = count_vowels + 1
        print(i)
    else:
        count_consonents = count_consonents + 1
        print(i)
        
print("The total number of vowels in the string is", count_vowels)
print("The total number of consonents in the string is", count_consonents)
